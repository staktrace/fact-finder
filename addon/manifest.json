{
    "manifest_version": 2,
    "name": "FactFinder",
    "version": "1.0",

    "description": "Allows training the fact finder AI",

    "content_scripts": [
        {
            "matches": "*",
            "js": ["collector.js"]
        }
    ],

    "background": {
        "scripts": ["submitter.js"]
    },

    "browser_action": {
        "default_icon": {
            "32": "icons/brain-32.png"
        }
    }
}
